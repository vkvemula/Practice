	public static MetadataService.Metadata returnMetadataTypeObject(String metadataType,String metadataString){
		MetadataService.Metadata mData = new MetadataService.Metadata();
		mData = (MetadataService.Metadata)JSON.deserialize(metadataString,Type.forName('MetadataService.'+metadataType));
		return mData;
		//return mTypeMap.get(metadataType);
	}
	
	public static Object returnMetadataTypeObject(String metadataType){
